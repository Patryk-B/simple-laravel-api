{
    "private": true,
    "type": "module",
    "scripts": {
        "//00": "",
        "dev": "vite",
        "build": "vite build",
        "//01": "",
        "__dev_windows_sail": "./vendor/bin/sail",
        "__dev_windows_sail_artisan": "bash ./vendor/laravel/sail/bin/sail artisan",
        "__dev_windows_sail_artisan_key_gen": "bash ./vendor/laravel/sail/bin/sail artisan key:generate --ansi",
        "__dev_windows_sail_artisan_migrate": "bash ./vendor/laravel/sail/bin/sail artisan migrate",
        "__dev_windows_sail_artisan_migrate_rollback": "bash ./vendor/laravel/sail/bin/sail artisan migrate:rollback",
