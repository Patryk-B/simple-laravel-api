        "__dev_windows_sail_artisan_migrate_fresh_seed": "bash ./vendor/laravel/sail/bin/sail artisan migrate:fresh --seed",
        "__dev_windows_sail_artisan_db_seed": "bash ./vendor/laravel/sail/bin/sail artisan db:seed",
        "__dev_windows_sail_up": "bash ./vendor/laravel/sail/bin/sail up --build --detach",
        "__dev_windows_sail_down": "bash ./vendor/laravel/sail/bin/sail down",
        "//02": "",
        "dev_windows_start": "npm run __dev_windows_sail_up",
        "dev_windows_stop": "npm run __dev_windows_sail_down",
        "//03": "",
        "__dev_linux_sail": "./vendor/bin/sail",
        "__dev_linux_sail_artisan": "./vendor/bin/sail artisan",
        "__dev_linux_sail_artisan_key_gen": "./vendor/bin/sail artisan key:generate --ansi",
