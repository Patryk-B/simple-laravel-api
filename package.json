        "__dev_linux_sail_artisan_migrate_fresh_seed": "./vendor/bin/sail artisan migrate:fresh --seed",
        "__dev_linux_sail_artisan_db_seed": "./vendor/bin/sail artisan db:seed",
        "__dev_linux_sail_up": "./vendor/bin/sail up --build --detach",
        "__dev_linux_sail_down": "./vendor/bin/sail up down",
        "//04": "",
